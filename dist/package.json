    "jinja/**/*.jinja",
    "js/**/*.d.ts",
    "js/**/*.{js,map}",
    "less/**/*.less",
    "md/**/*.md",
    "oco/**/*.oco",
    "png/**/*.png",
    "py/**/*.py",
    "rcpx/**/*.rcpx",
    "scss/**/*.scss",
    "sketchpalette/**/*.sketchpalette",
    "styl/**/*.styl",
    "svg/**/*.svg",
    "tex/**/*.tex",
    "ts/**/*.ts",
    "!.DS_Store"
