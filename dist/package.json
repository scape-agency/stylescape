{
  "name": "stylescape",
  "version": "0.0.23",
  "description": "Stylescape is a visual identity framework developed by Scape Agency.",
  "keywords": [
    "stylescape",
    "sass",
    "responsive",
    "front-end",
    "layout-engine",
    "framework",
    "web"
  ],
  "license": "MIT",
  "homepage": "https://www.scape.style/",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/scape-foundation"
    }
  ],
  "type": "module",
  "main": "js/index",
  "types": "js/index",
  "files": [
